{
  "name": "christopherbiscardi.github.com",
  "version": "1.0.0",
  "description": "my website",
  "main": "index.js",
  "repository": "git@github.com:ChristopherBiscardi/christopherbiscardi.github.com.git",
  "author": "ChristopherBiscardi <chris@christopherbiscardi.com>",
  "license": "MIT",
  "scripts": {
    "precommit": "lint-staged",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "update-sens8": "git subtree pull --prefix sens8 git@github.com:ChristopherBiscardi/sens8.git master --squash"
  },
  "private": true,
  "workspaces": ["./sens8/packages/*", "."],
  "lint-staged": {
    "*.{js,jsx}": ["prettier --parser flow --write", "git add"],
    "*.json": ["prettier --parser json --write", "git add"],
    "*.{graphql,gql}": ["prettier --parser graphql --write", "git add"],
    "*.{md,markdown}": ["prettier --parser markdown --write", "git add"],
    "*.{css,scss}": ["prettier --parser css --write", "git add"]
  },
  "dependencies": {
    "@sens8/component-typography": "*",
    "emotion": "^9.2.6",
    "emotion-server": "^9.2.6",
    "emotion-theming": "^9.2.6",
    "gatsby": "next",
    "gatsby-mdx": "0.1.5-ci",
    "gatsby-plugin-emotion": "next",
    "gatsby-plugin-google-analytics": "next",
    "gatsby-plugin-offline": "next",
    "gatsby-plugin-react-helmet": "next",
    "gatsby-remark-prismjs": "next",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "palx": "^1.0.3",
    "prettier": "^1.14.2",
    "prismjs": "^1.14.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-emotion": "^9.2.6",
    "react-helmet": "^5.2.0",
    "react-pose": "^3.3.0",
    "react-prism": "^4.3.2",
    "slugify": "^1.3.1",
    "styled-system": "^3.0.1",
    "superbox": "^2.1.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@mdx-js/loader": "^0.16.1",
    "@mdx-js/mdx": "^0.16.1",
    "gatsby-source-filesystem": "next",
    "loader-utils": "^1.1.0"
  }
}
